{
    "model": "ResNet50",
    "pretrained": true,
    "input_info": {
        "sample_size": [1, 3, 224, 224]
    },

    "batch_size": 256,
    "epochs": 1,
    "distributed": true,

    "optimizer": {
        "type": "momentum",
        "momentum": 0.9,
        "decay": 0.9,
        "epsilon": 0.001,
        "schedule_type": "piecewise_constant_with_warmup",
        "schedule_params": {
            "boundaries": [30, 60, 80],
            "multipliers": [1.0, 0.1, 0.01, 0.001],
            "warmup_epochs": 5
        }
    },

    "dataset": "imagenet2012",
    "dataset_type": "tfrecords"
}
