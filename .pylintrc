[MASTER]
disable = arguments-differ,
          fixme,
          invalid-name,
          logging-format-interpolation,
          missing-docstring,
          no-self-use,
          too-few-public-methods,
max-line-length = 120
ignore-docstrings = yes

[SIMILARITIES]
ignore-imports = yes

[BASIC]
bad-functions = print
good-names = logger,fn

[DESIGN]
max-statements=60
max-branches=13